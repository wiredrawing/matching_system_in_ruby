<p>ここはマイページ編集ページ</p>
<p><%= @current_user.display_name %></p>
<p><%= @current_user.email %></p>
<p><%= @current_user.password_digest %></p>
<%= form_with(:model => @current_user, :url => mypage_edit_path) do | form | %>
  <div>
    <p>フォーム</p>
    <%= form  %>
    <%= form.hidden_field(:id) %>
    <p><%= form.text_field(:email, {:readonly => true }) %></p>
    <p><%= form.text_field(:display_name) %></p>
    <p><%= form.text_field(:family_name) %></p>
    <p><%= form.text_field(:given_name) %></p>
    <p>パスワード<br>
      <%= form.text_field(:password) %>
    </p>
    <p>パスワード<br>
      <%= form.text_field(:password_confirmation) %>
    </p>
    <p><%= form . submit(:update, {
      :value => "更新",
      :id => "button-to-update",
      :class => "button-to-update",
      }) %></p>
  </div>
<% end %>
