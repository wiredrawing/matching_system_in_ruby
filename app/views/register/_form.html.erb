<%= form_with({
  url: register_create_path,
  model: member,
  method: "post",
}) do |form| %>
  <% if member.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(member.errors.count, "error") %> prohibited this member from being saved:</h2>
      <ul>
        <% member.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="actions">
    <p><%= form.text_field :email, {:readonly => true} %></p>
    <p><%= form.text_field :display_name, { :readonly => true } %></p>
    <p><%= form.text_field :family_name %></p>
    <p><%= form.select :gender, @genders %></p>
    <p><%= form.text_field :given_name %></p>
    <p><%= form.text_field :message %></p>
    <p><%= form.text_field :memo %></p>
    <p>
      パスワード<br>
      <%= form.text_field :password %>
    </p>
    <p>
      確認用パスワード<br>
      <%= form.text_field :password_confirmation %>
    </p>
    <!-- 本登録用トークン -->
    <%= form.hidden_field :token %>
    <!-- 仮登録ユーザーID -->
    <%= form.hidden_field :id %>
    <p><%= form.submit %>
    </div>
  <% end %>
